{
  "manifest_version": 3,
  "name": "BreakLoop",
  "description": "Replace YouTube videos with random boring ones.",
  "version": "1.1",
  "permissions": ["tabs", "webRequest"],
  "host_permissions": [
    "*://www.youtube.com/*",
    "*://youtube.com/*"
  ],
  "icons": {
    "16": "Images/Icon16.png",
    "32": "Images/Icon32.png",
    "48": "Images/Icon48.png",
    "128": "Images/Icon128.png"
  },
  "background": {
    "service_worker": "background.js"
    
  }
}
